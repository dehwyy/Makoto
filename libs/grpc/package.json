{
  "name": "@makoto/grpc",
  "type": "module",
  "private": true,
  "exports": {
		".": {
      "types": "./dist/types/lib/index.d.ts",
			"import": "./dist/lib/index.js"
		},
    "./clients": {
      "types": "./dist/types/lib/clients/index.d.ts",
      "import": "./dist/lib/clients/index.js"
    }
	},

  "typesVersions": {
    "*": {
      "clients":["./dist/types/lib/clients/index.d.ts"]
    }
  },
  "types": "./dist/types/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:js": "tsc",
    "generate:types": "tsc --declaration true --emitDeclarationOnly --outDir dist/types",
    "ts": "bun run generate:js && bun run generate:types"
  },
  "dependencies": {
    "@protobuf-ts/plugin": "^2.9.1",
     "@protobuf-ts/runtime-rpc": "^2.9.1"
  }
}
